{
  "name": "@grazulex/vaultmark",
  "version": "1.1.0",
  "description": "Ephemeral credential manager for secure AI-assisted development",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "vaultmark": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc && node scripts/fix-imports.mjs",
    "check": "biome check .",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run check && npm run build",
    "install:global": "npm run build && npm link",
    "uninstall:global": "npm unlink -g @grazulex/vaultmark"
  },
  "keywords": [
    "ssh",
    "credentials",
    "ephemeral",
    "security",
    "certificate",
    "cli",
    "vault"
  ],
  "author": "Grazulex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/grazulex/vaultmark.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "inquirer": "^12.2.0",
    "ora": "^8.1.1",
    "ssh2": "^1.16.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.1",
    "@types/ssh2": "^1.15.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.7"
  }
}
